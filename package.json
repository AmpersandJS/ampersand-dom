{
  "name": "ampersand-dom",
  "description": "Super light-weight DOM manipulation lib.",
  "version": "1.4.0",
  "author": "'Henrik Joreteg' <henrik@andyet.net>",
  "bugs": {
    "url": "https://github.com/ampersandjs/ampersand-dom/issues"
  },
  "devDependencies": {
    "browserify": "~4.1.8",
    "phantomjs": "^1.9.7-15",
    "run-browser": "~1.3.1",
    "tap-spec": "^0.2.0",
    "tape": "^2.13.2",
    "tape-run": "~0.1.1",
    "tape-suite": "^0.2.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/ampersandjs/ampersand-dom",
  "keywords": [
    "dom",
    "binding"
  ],
  "license": "MIT",
  "main": "ampersand-dom.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ampersandjs/ampersand-dom"
  },
  "scripts": {
    "start": "run-browser test/index.js",
    "test": "browserify test/index.js | tape-run | tap-spec"
  },
  "testling": {
    "files": "test/index.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "browserify": {
    "transform": [
      "ampersand-version"
    ]
  },
  "dependencies": {
    "ampersand-version": "^1.0.0"
  }
}
